#include "tegra186-sunsteaker-slt-spmic-e3340-1099-a00-00.dtsi"
#include "tegra186-sunsteaker-slt-fixed-e3340-1099-a00-00.dtsi"
#include "tegra186-cvb-slt-p2598-a00-e3340-1099-a00-00.dtsi"

/ {
	ether_qos@2490000 {
		vddio_sys_enet_bias-supply = <&spmic_sd2>;
		vddio_enet-supply = <&spmic_sd3>;
		phy_vdd_1v8-supply = <&spmic_sd2>;
		phy_ovdd_rgmii-supply = <&spmic_sd3>;
		phy_pllvdd-supply = <&spmic_sd1>;
	};

        host1x {
		nvdisplay@15200000 {
			avdd_lcd-supply = <&vdd_dis_3v3_lcd>;
			dvdd_lcd-supply = <&vdd_lcd_1v8_dis>;
			avdd_dsi_csi-supply = <&spmic_sd1>;
                        avdd_hdmi-supply = <&spmic_ldo7>;
                        avdd_hdmi_pll-supply = <&spmic_sd2>;
                        vdd_hdmi_5v0-supply = <&vdd_hdmi0_5v0>;
		};
                nvdisplay@15210000 {
                        avdd_hdmi-supply = <&spmic_ldo7>;
                        avdd_hdmi_pll-supply = <&spmic_sd2>;
                        vdd_hdmi_5v0-supply = <&vdd_hdmi0_5v0>;
                };
	};

	sdhci@3460000 {
		vddio_sdmmc-supply = <&spmic_sd2>;
		vddio_sd_slot-supply = <&spmic_sd3>;
		vqmmc-supply = <&spmic_sd2>;
		vmmc-supply = <&spmic_sd3>;
	};

	sdhci@3440000 {
		vddio_sdmmc-supply = <&spmic_ldo5>;
		vddio_sd_slot-supply = <&vdd_sdcard1_3v3>;
		vqmmc-supply = <&spmic_ldo5>;
		vmmc-supply = <&vdd_sdcard1_3v3>;
	};

	sdhci@3420000 {
		vddio_sdmmc-supply = <&spmic_sd2>;
		vddio_sd_slot-supply = <&spmic_ldo6>;
		vqmmc-supply = <&spmic_sd2>;
		vmmc-supply = <&spmic_ldo6>;
	};

	sdhci@3400000 {
		vddio_sdmmc-supply = <&spmic_ldo3>;
		vddio_sd_slot-supply = <&vdd_sdcard1_3v3>;
		vqmmc-supply = <&spmic_ldo3>;
		vmmc-supply = <&vdd_sdcard1_3v3>;
	};

	ahci-sata@3507000 {
		target-3v3-supply = <&vdd_3v3_sata>;
	};

	xhci@3530000 {
		avddio_usb-supply = <&spmic_sd3>;
		avdd_pll_utmip-supply = <&spmic_ldo0>;
		hvdd_usb-supply =  <&vdd_1v8_sw>;
	};

	xudc@3550000 {
		avdd-usb-supply = <&spmic_sd3>;
		dvdd-pex-supply = <&spmic_ldo7>;
		hvdd-pex-supply =  <&vdd_1v8_sw>;
		dvdd-pex-pll-supply = <&spmic_ldo7>;
		hvdd-pex-pll-supply = <&vdd_1v8_sw>;
	};

	pinctrl@3520000 {
		vbus-0-supply = <&vdd_usb0_5v>;
		vbus-1-supply = <&vdd_usb1_5v>;
		vbus-2-supply = <&vdd_usb2_5v>;
		vbus-3-supply = <&battery_reg>;
		vddio-hsic-supply = <&spmic_sd1>;
		hvdd_pex_pll-supply = <&vdd_1v8_sw>;
		hvdd_pex-supply = <&vdd_1v8_sw>;
		dvdd_pex-supply = <&spmic_ldo7>;
		avdd_usb-supply = <&spmic_sd3>;
		vclamp_usb-supply = <&vdd_1v8_sw>;
		avdd_pll_erefeut-supply = <&spmic_ldo0>;
	};

	pcie-controller@10003000 {
		dvdd-pex-supply = <&spmic_ldo7>;
		hvdd-pex-pll-supply = <&spmic_sd2>;
		hvdd-pex-supply = <&spmic_sd2>;
		vddio-pexctl-aud-supply = <&spmic_sd2>;
	};

	pmc-iopower {
		iopower-sys-supply = <&spmic_ldo6>;
		iopower-uart-supply = <&vdd_1v8_sw>;
		iopower-conn-supply = <&vdd_1v8_sw>;
		iopower-edp-supply = <&vdd_1v8_sw>;
		iopower-pex-ctrl-audio-supply = <&vdd_1v8_sw>;
		iopower-ufs-supply = <&spmic_sd1>;
		iopower-ddr0-supply = <&spmic_sd0>;
		iopower-ddr1-supply = <&spmic_sd0>;
		iopower-csi-dsi-supply = <&spmic_sd1>;
		iopower-cam-supply = <&vdd_1v8_sw>;
		iopower-sdmmc4-supply = <&vdd_1v8_sw>;
		iopower-sdmmc1-hv-supply = <&spmic_ldo3>;
		iopower-audio-hv-supply = <&spmic_sd3>;
		iopower-dbg-supply = <&spmic_ldo6>;
		iopower-spi-supply = <&vdd_1v8_sw>;
		iopower-ao-supply = <&spmic_ldo6>;
		iopower-ao-hv-supply = <&spmic_ldo2>;
		iopower-dmic-hv-supply =  <&spmic_sd3>;
		iopower-sdmmc2-hv-supply = <&spmic_sd3>;
		iopower-sdmmc3-hv-supply =  <&spmic_ldo5>;
	};

	i2c@c250000 {
		tegra_nct72: onsemi,nct72@4c {
			vdd-supply = <&vdd_1v8_sw>;
		};
	};

	bpmp_i2c {
		spmic@3c {

			pinmux@0 {
				pin_gpio1 {
					maxim,active-fps-power-up-slot = <7>;
					maxim,active-fps-power-down-slot = <0>;
				};

				pin_gpio2 {
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <1>;
				};

				pin_gpio3 {
					maxim,active-fps-power-up-slot = <1>;
					maxim,active-fps-power-down-slot = <7>;
				};
			};

			regulators {
				sd0 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <5>;
					maxim,active-fps-power-down-slot = <1>;
					regulator-enable-ramp-delay = <220>;
					regulator-disable-ramp-delay = <11000>;
					regulator-ramp-delay = <27500>;
					regulator-ramp-delay-scale = <550>;
				};

				sd1 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <2>;
					maxim,active-fps-power-down-slot = <3>;
					regulator-enable-ramp-delay = <235>;
					regulator-disable-ramp-delay = <6000>;
					regulator-ramp-delay = <27500>;
					regulator-ramp-delay-scale = <550>;
				};

				sd2 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <3>;
					maxim,active-fps-power-down-slot = <4>;
					regulator-enable-ramp-delay = <370>;
					regulator-disable-ramp-delay = <11000>;
					regulator-ramp-delay = <27500>;
					regulator-ramp-delay-scale = <600>;
				};

				sd3 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <5>;
					maxim,active-fps-power-down-slot = <2>;
					regulator-enable-ramp-delay = <400>;
					regulator-disable-ramp-delay = <16000>;
					regulator-ramp-delay = <27500>;
					regulator-ramp-delay-scale = <350>;
				};

				ldo0 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <3>;
					maxim,active-fps-power-down-slot = <4>;
					regulator-enable-ramp-delay = <1000>;
					regulator-disable-ramp-delay = <16000>;
					regulator-ramp-delay = <5000>;
					regulator-ramp-delay-scale = <350>;
				};

				ldo1 {
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-ramp-delay = <5000>;
				};

				ldo2 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <5>;
					maxim,active-fps-power-down-slot = <2>;
					regulator-enable-ramp-delay = <1900>;
					regulator-disable-ramp-delay = <9000>;
					regulator-ramp-delay = <5000>;
					regulator-ramp-delay-scale = <300>;
				};

				ldo3 {
					maxim,active-fps-power-up-slot = <7>;
					maxim,active-fps-power-down-slot = <0>;
					regulator-enable-ramp-delay = <1900>;
					regulator-disable-ramp-delay = <5000>;
					regulator-ramp-delay = <5000>;
					regulator-ramp-delay-scale = <300>;
				};

				ldo4 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-enable-ramp-delay = <23>;
					regulator-disable-ramp-delay = <4000>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <250>;
				};

				ldo5 {
					maxim,active-fps-power-up-slot = <7>;
					maxim,active-fps-power-down-slot = <0>;
					regulator-enable-ramp-delay = <2000>;
					regulator-disable-ramp-delay = <9000>;
					regulator-ramp-delay = <5000>;
					regulator-ramp-delay-scale = <350>;
				};

				ldo6 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <2>;
					maxim,active-fps-power-down-slot = <5>;
					regulator-enable-ramp-delay = <1100>;
					regulator-disable-ramp-delay = <19000>;
					regulator-ramp-delay = <5000>;
					regulator-ramp-delay-scale = <350>;
				};

				ldo7 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <4>;
					maxim,active-fps-power-down-slot = <3>;
					regulator-enable-ramp-delay = <570>;
					regulator-disable-ramp-delay = <11000>;
					regulator-ramp-delay = <5000>;
					regulator-ramp-delay-scale = <300>;
				};

				ldo8 {
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-ramp-delay = <5000>;
				};
			};
		};
	};

	fixed-regulators {

		regulator@2 {
			regulator-enable-ramp-delay = <4000>;
			regulator-disable-ramp-delay = <10000>;
		};

		regulator@3 {
			regulator-enable-ramp-delay = <425>;
			regulator-disable-ramp-delay = <27000>;
		};

		regulator@4 {
			regulator-enable-ramp-delay = <1100>;
			regulator-disable-ramp-delay = <1900>;
		};

		regulator@5 {
			regulator-enable-ramp-delay = <1350>;
		};

		regulator@6 {
			regulator-enable-ramp-delay = <1300>;
		};

		regulator@7 {
			regulator-enable-ramp-delay = <350>;
			regulator-disable-ramp-delay = <7500>;
		};

		regulator@9 {
			regulator-enable-ramp-delay = <255>;
			regulator-disable-ramp-delay = <99>;
		};

		regulator@13 {
			regulator-enable-ramp-delay = <3450>;
			regulator-disable-ramp-delay = <3500>;
		};

		regulator@14 {
			regulator-enable-ramp-delay = <3300>;
			regulator-disable-ramp-delay = <17000>;
		};
	};
};
